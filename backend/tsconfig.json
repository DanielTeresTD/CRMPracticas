{
    "compilerOptions": {
        "target": "es2016", // Version ts used when compiled
        "module": "commonjs",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
